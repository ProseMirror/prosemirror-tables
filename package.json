  "description": "ProseMirror's rowspan/colspan tables component",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "style": "style/tables.css",
  "typings": "./index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/prosemirror/prosemirror-tables.git"
  },
  "maintainers": [
    {
      "name": "Eduard Shvedai",
      "email": "eshvedai@atlassian.com"
    },
    {
      "name": "Huanhuan Huang",
      "email": "hhuang@atlassian.com"
    }
  ],
  "dependencies": {
    "prosemirror-keymap": "^1.1.2",
    "prosemirror-model": "^1.8.1",
    "prosemirror-state": "^1.3.1",
    "prosemirror-transform": "^1.2.1",
    "prosemirror-view": "1.23.2"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.20.0",
    "@types/prosemirror-model": "^1.13.2",
    "@types/prosemirror-state": "^1.2.8",
    "builddocs": "^0.3.0",
    "ist": "1.1.7",
    "jsdom": "^18.1.0",
    "mocha": "9.1.3",
    "mocha-jsdom": "^2.0.0",
    "prosemirror-commands": "^1.1.12",
    "prosemirror-example-setup": "^1.1.2",
    "prosemirror-menu": "^1.1.4",
    "prosemirror-schema-basic": "^1.1.2",
    "prosemirror-test-builder": "^1.0.5",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^4.5.2",
    "typings-tester": "^0.3.2"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "mocha test/test-*.js",
    "test-performance": "node --allow-natives-syntax ./node_modules/mocha/bin/_mocha --reporter mocha-performance test/test-*.js",
    "build_demo": "rollup -c rollup.demo.config.js",
    "watch_demo": "rollup -w -c rollup.demo.config.js",
    "build_demo_large": "rollup -c rollup.demo.large.config.js",
    "watch_demo_large": "rollup -w -c rollup.demo.large.config.js",
    "build": "rollup -c",
    "watch": "rollup -w -c",
    "prepare": "npm run build",
    "build_readme": "builddocs --name tables --format markdown --main src/README.md src/*.js > README.md"
  }
}
