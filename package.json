{
  "name": "prosemirror-tables",
  "version": "1.1.1",
  "description": "ProseMirror's rowspan/colspan tables component",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "style": "style/tables.css",
  "typings": "./index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/prosemirror/prosemirror-tables.git"
  },
  "maintainers": [
    {
      "name": "Eduard Shvedai",
      "email": "eshvedai@atlassian.com"
    },
    {
      "name": "Huanhuan Huang",
      "email": "hhuang@atlassian.com"
    }
  ],
  "files": [
    "src/",
    "dist/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "index.d.ts",
    "package.json"
  ],
  "dependencies": {
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.17.0",
    "prosemirror-state": "^1.4.0",
    "prosemirror-transform": "^1.5.0",
    "prosemirror-view": "^1.25.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "builddocs": "^0.3.0",
    "ist": "^1.1.1",
    "mocha": "^3.4.2",
    "prosemirror-commands": "^1.3.0",
    "prosemirror-example-setup": "^1.2.0",
    "prosemirror-menu": "^1.2.0",
    "prosemirror-schema-basic": "^1.2.0",
    "prosemirror-test-builder": "^1.1.0",
    "rollup": "^2.75.5",
    "typescript": "^4.0.0",
    "typings-tester": "^0.3.2"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "mocha test/test-*.js",
    "build_demo": "rollup -c rollup.demo.config.js",
    "watch_demo": "rollup -w -c rollup.demo.config.js",
    "build": "rollup -c",
    "watch": "rollup -w -c",
    "prepare": "npm run build",
    "build_readme": "builddocs --name tables --format markdown --main src/README.md src/*.js > README.md"
  }
}
