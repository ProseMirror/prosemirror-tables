{
  "name": "@policyco/prosemirror-tables",
  "version": "1.2.1",
  "description": "PolicyCo's version of ProseMirror's rowspan/colspan tables component",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "style": "style/tables.css",
  "typings": "./index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/policyco/prosemirror-tables.git"
  },
  "maintainers": [],
  "dependencies": {
    "prosemirror-keymap": "^1.1.5",
    "prosemirror-model": "^1.16.1",
    "prosemirror-state": "^1.3.4",
    "prosemirror-transform": "^1.3.4",
    "prosemirror-view": "^1.23.8"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@types/prosemirror-model": "^1.16.1",
    "@types/prosemirror-state": "^1.2.8",
    "builddocs": "^0.3.0",
    "ist": "^1.1.7",
    "mocha": "^9.2.1",
    "prosemirror-commands": "^1.2.1",
    "prosemirror-example-setup": "^1.1.2",
    "prosemirror-menu": "^1.1.4",
    "prosemirror-schema-basic": "^1.1.2",
    "prosemirror-test-builder": "^1.0.5",
    "rollup": "^2.70.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^4.5.2",
    "typings-tester": "^0.3.2"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "mocha test/test-*.js",
    "build_demo": "rollup -c rollup.demo.config.js",
    "watch_demo": "rollup -w -c rollup.demo.config.js",
    "build": "rollup -c",
    "watch": "rollup -w -c",
    "prepare": "npm run build",
    "build_readme": "builddocs --name tables --format markdown --main src/README.md src/*.js > README.md"
  }
}
